loan_amounts = []
for amount in loan_amounts:
    try:
        loan_amounts.append(float(amount))
    except ValueError:  # Ignorer les valeurs non convertibles
        continue

loan_amounts = np.array(loan_amounts)

# Calculer les statistiques
mean_loan = np.mean(loan_amounts)
median_loan = np.median(loan_amounts)
std_loan = np.std(loan_amounts)

# Afficher les résultats
print(f"Moyenne des montants des prêts : {mean_loan}")
print(f"Médiane des montants des prêts : {median_loan}")
print(f"Écart type des montants des prêts : {std_loan}")
